{
    "compilerOptions": {
      "jsx": "react",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./", 
      "paths": { 
        "components/*": ["src/components/*"] 
      } 
    },
    "include": [
      "src/**/*",
      "src/custom.d.ts"  // Ensure your custom declaration file is included
    ]
  }
  